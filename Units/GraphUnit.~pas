unit GraphUnit;

interface

uses
  listOfPointersUnit;

type
  TVertex = record
    latitude, longitude: real; // широта, долгота
    goList: TListOfPointers;   // вершины, в которые есть ребро
  end;
  TEdge = record
    movingType: (car, plane, foot);
    weight: real;              
  end;
  TGraphList = TListOfPointers;

implementation

end.
